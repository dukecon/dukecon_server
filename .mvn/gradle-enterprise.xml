<gradleEnterprise
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://www.gradle.com/gradle-enterprise-maven">
    <server>
        <id>dukecon</id>
        <url>https://gradle.dukecon.org</url>
        <allowUntrusted>true</allowUntrusted>
    </server>
    <buildScan>
        <publish>ALWAYS</publish>
        <captureGoalInputFiles>true</captureGoalInputFiles>
        <backgroundBuildScanUpload>#{isFalse(env["DOCKER"])}</backgroundBuildScanUpload>
    </buildScan>
    <buildCache>
        <remote>
            <enabled>true</enabled>
        </remote>
        <local>
            <enabled>true</enabled>
        </local>
    </buildCache>
</gradleEnterprise>
