      "_source":{"ID_KONGRESS":522447,"ID":529408,"ID_SEMINAR":"529408","FARBCODE":336732,"TRACK":"Internet der Dinge","TRACK_EN":"Internet of Things","ORDERT":1,"AUDIENCE":"Anfänger","AUDIENCE_EN":"Beginners","DATUM":"2017-03-28T00:00:00.000+02:00","SIMULTAN":"0","AUSGEBUCHT":1,"DATUM_ES_EN":"2017-03-28","DATUM_ES":"28.03.2017","BEGINN":"12:00","ENDE":"12:40","TIMESTAMP":"0017-03-26T11:53:28.000+00:53:28","TIMESTAMP_ENDE":"0017-03-26T12:33:28.000+00:53:28","SEMINAR_NR":"47","RAUM_NR":"7","RAUMNAME":"Neptun","SITZPLATZ":"120","AREAID":"7/N","TITEL":"Hack deine Zahnbürste!","TITEL_ES":"Hack deine Zahnbürste!","TITEL_EN":null,"ABSTRACT_EN":null,"REFERENT_NAME":"Anton Epple","REFERENT_VORNAME":"Anton","COREFERENT_VORNAME":null,"COCOREFERENT_VORNAME":null,"COREFERENT_NACHNAME":null,"COCOREFERENT_NACHNAME":null,"KEYWORDS":",IoT (Internet of Things),Rasberry Pi (Einplatinencomputer)","REFERENT_NACHNAME":"Epple","REFERENT_FIRMA":"Dukehoff GmbH","ID_PERSON":375163,"ID_PERSON_COREF":null,"ID_PERSON_COCOREF":null,"VORTRAGSTYP":"Projektbericht","VORTRAGSTYP_EN":"Project report","COREFERENT_NAME":null,"COCOREFERENT_NAME":null,"COREFERENT_FIRMA":null,"COCOREFERENT_FIRMA":null,"ABSTRACT_TEXT":"&quot;Wie viele Entwickler braucht man, um eine Glühbirne zu wechseln?&quot; - &quot;Keinen, das ist ein Hardwareproblem!&quot; Ist das immer noch so? Java-Entwickler von heute überwinden ganz locker die Software/Hardware-Barriere. Mit Mini-Computern wie dem Raspberry Pi und modernen IDEs und APIs ist das überraschend einfach geworden. In dieser Session hacken wir alles, was uns in den Weg kommt, die Zahnbürste in der Cloud, Nähmaschinen und sogar einen Badezimmerspiegel.","SPRACHE":"Deutsch","DEMO":"Ja","KEYWORDS_EN":",IoT (Internet of Things),Rasberry Pi (single board computer)","SPRACHE_EN":"German","DEMO_EN":"yes","BEGINN_EN":null,"ENDE_EN":null}
