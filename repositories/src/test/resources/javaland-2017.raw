      "_source":{"ID_KONGRESS":522447,"ID":529321,"ID_SEMINAR":"529321","FARBCODE":336732,"TRACK":"Enterprise Java & Cloud","TRACK_EN":"Enterprise Java & Cloud","ORDERT":1,"AUDIENCE":"Anfänger","AUDIENCE_EN":"Beginners","DATUM":"2017-03-28T00:00:00.000+02:00","SIMULTAN":"0","AUSGEBUCHT":0,"DATUM_ES_EN":"2017-03-28","DATUM_ES":"28.03.2017","BEGINN":"12:00","ENDE":"12:40","TIMESTAMP":"0017-03-26T11:53:28.000+00:53:28","TIMESTAMP_ENDE":"0017-03-26T12:33:28.000+00:53:28","SEMINAR_NR":"26","RAUM_NR":"4","RAUMNAME":"Quantum 1+2","SITZPLATZ":"250","AREAID":"4/Q","TITEL":"Java EE 8 - Java EE Security API ","TITEL_ES":"Java EE 8 - Java EE Security API ","TITEL_EN":null,"ABSTRACT_EN":null,"REFERENT_NAME":"Rudy De Busscher","REFERENT_VORNAME":"Rudy","COREFERENT_VORNAME":null,"COCOREFERENT_VORNAME":null,"COREFERENT_NACHNAME":null,"COCOREFERENT_NACHNAME":null,"KEYWORDS":",Java Enterprise Edition (Java EE)","REFERENT_NACHNAME":"De Busscher","REFERENT_FIRMA":"C4J","ID_PERSON":374706,"ID_PERSON_COREF":null,"ID_PERSON_COCOREF":null,"VORTRAGSTYP":"Neuerscheinung oder Feature","VORTRAGSTYP_EN":"New release or feature","COREFERENT_NAME":null,"COCOREFERENT_NAME":null,"COREFERENT_FIRMA":null,"COCOREFERENT_FIRMA":null,"ABSTRACT_TEXT":"The presentation gives an overview of the current status of the Java EE Security API work, part of the upcoming Java EE 8 specification.\r\n\r\nBy starting why there is a need for such a new specification, we will  cover the main concepts related to security.\r\nSoteria is the reference implementation of the specification. Although the spec is targetted to Java E8, we can use it in several Application servers already. Various common use cases will be presented in a demo.\r\n\r\nDevelopers feedback is important to reach a good specification which can please the developers. At the end, there will be room for questions, remarks, feedback, etc...","SPRACHE":"Englisch","DEMO":"Ja","KEYWORDS_EN":",Java Enterprise Edition (Java EE)","SPRACHE_EN":"English","DEMO_EN":"yes","BEGINN_EN":null,"ENDE_EN":null}
