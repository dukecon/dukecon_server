      "_source":{"ID_KONGRESS":522447,"ID":529587,"ID_SEMINAR":"529587","FARBCODE":336732,"TRACK":"IDE & Tools","TRACK_EN":"IDE & Tools","ORDERT":1,"AUDIENCE":"Anfänger","AUDIENCE_EN":"Beginners","DATUM":"2017-03-29T00:00:00.000+02:00","SIMULTAN":"0","AUSGEBUCHT":0,"DATUM_ES_EN":"2017-03-29","DATUM_ES":"29.03.2017","BEGINN":"12:00","ENDE":"12:40","TIMESTAMP":"0017-03-27T11:53:28.000+00:53:28","TIMESTAMP_ENDE":"0017-03-27T12:33:28.000+00:53:28","SEMINAR_NR":"89","RAUM_NR":"6","RAUMNAME":"Eventhalle","SITZPLATZ":"400","AREAID":"6/E","TITEL":"Code Quality Metrics Demystified","TITEL_ES":"Code Quality Metrics Demystified","TITEL_EN":null,"ABSTRACT_EN":null,"REFERENT_NAME":"Jeroen Resoort","REFERENT_VORNAME":"Jeroen","COREFERENT_VORNAME":null,"COCOREFERENT_VORNAME":null,"COREFERENT_NACHNAME":null,"COCOREFERENT_NACHNAME":null,"KEYWORDS":null,"REFERENT_NACHNAME":"Resoort","REFERENT_FIRMA":"JDriven","ID_PERSON":374949,"ID_PERSON_COREF":null,"ID_PERSON_COCOREF":null,"VORTRAGSTYP":"Best Practices","VORTRAGSTYP_EN":"Best practices","COREFERENT_NAME":null,"COCOREFERENT_NAME":null,"COREFERENT_FIRMA":null,"COCOREFERENT_FIRMA":null,"ABSTRACT_TEXT":"In our everyday work as developers, we are confronted with various software metrics. What do these numbers mean, how are they calculated? Do these metrics enable us to make better software?\r\n\r\nIn this talk we investigate the use of software metrics in general and also take a detailed look at several frequently used code quality metrics. We will learn about the meaning and usefulness of metrics such as lines of code, cyclomatic complexity, duplication, package entanglement and unit test coverage.","SPRACHE":null,"DEMO":null,"KEYWORDS_EN":null,"SPRACHE_EN":null,"DEMO_EN":null,"BEGINN_EN":null,"ENDE_EN":null}
