      "_source":{"ID_KONGRESS":522447,"ID":539087,"ID_SEMINAR":"539087","FARBCODE":336732,"TRACK":"Core Java & JVM-Sprachen","TRACK_EN":"Core Java & JVM-Languages","ORDERT":1,"AUDIENCE":"Anfänger","AUDIENCE_EN":"Beginners","DATUM":"2017-03-28T00:00:00.000+02:00","SIMULTAN":"0","AUSGEBUCHT":0,"DATUM_ES_EN":"2017-03-28","DATUM_ES":"28.03.2017","BEGINN":"16:00","ENDE":"16:40","TIMESTAMP":"0017-03-26T15:53:28.000+00:53:28","TIMESTAMP_ENDE":"0017-03-26T16:33:28.000+00:53:28","SEMINAR_NR":"034789621450","RAUM_NR":"7","RAUMNAME":"Neptun","SITZPLATZ":"120","AREAID":"7/N","TITEL":"Java APIs you should know","TITEL_ES":"Java APIs you should know","TITEL_EN":null,"ABSTRACT_EN":null,"REFERENT_NAME":"Hendrik Ebbers","REFERENT_VORNAME":"Hendrik","COREFERENT_VORNAME":null,"COCOREFERENT_VORNAME":null,"COREFERENT_NACHNAME":null,"COCOREFERENT_NACHNAME":null,"KEYWORDS":null,"REFERENT_NACHNAME":"Ebbers","REFERENT_FIRMA":"Canoo Engineering AG","ID_PERSON":364422,"ID_PERSON_COREF":null,"ID_PERSON_COCOREF":null,"VORTRAGSTYP":"Best Practices","VORTRAGSTYP_EN":"Best practices","COREFERENT_NAME":null,"COCOREFERENT_NAME":null,"COREFERENT_FIRMA":null,"COCOREFERENT_FIRMA":null,"ABSTRACT_TEXT":"This isn't a talk about microservices, NO-SQL, Container solutions or hip new frameworks. This talk will show some of the standard Java APIs that are part of Java since version 5, 6, 7 or 8. All this features are very helpful to create maintainable and future-proof applications, regardless of whether JavaEE, Spring, JavaFX or any other framework is used. The talk will give an overview of some important standard concepts and APIs of Java like annotations, null values and concurrency. Based on an overview of this topics and some samples the talk will answer questions like: How can I create my own annotations? How can I create a plugin structure without using frameworks like OSGI? What's the best way to handle NullPointerExceptions? How can I write concurrent code that is still maintainable?","SPRACHE":"Deutsch","DEMO":"Ja","KEYWORDS_EN":null,"SPRACHE_EN":"German","DEMO_EN":"yes","BEGINN_EN":null,"ENDE_EN":null}
